{
  "name": "marketplace-app-boilerplate",
  "version": "1.2.3",
  "private": true,
  "files": [
    "dist"
  ],
  "dependencies": {
    "@contentstack/app-sdk": "^1.5.0",
    "@contentstack/venus-components": "^2.1.0",
    "axios": "^1.3.4",
    "form-data": "^4.0.0",
    "html-react-parser": "^3.0.8",
    "jsonfile": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "sass": "^1.65.1",
    "web-vitals": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --verbose",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "prettify": "prettier --write .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx . --ignore-pattern './node_modules/' --ignore-pattern './build/' --ignore-pattern '.eslintrc.js'",
    "precommit": "npm run prettify && npm run lint:fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.4.0",
    "@playwright/test": "^1.23.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.182",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "playwright": "^1.23.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.9.5"
  }
}
