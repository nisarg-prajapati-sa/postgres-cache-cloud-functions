{
  "name": "@contentstack/rte-plugin-boilerplate",
  "version": "1.0.0",
  "description": "An initial boilerplate for creating a new RTE plugin",
  "entry": "plugin",
  "output": "json-rte.js",
  "main": "dist/json-rte.js",
  "typings": "dist/types/json-rte.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack serve --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js",
    "build-academy": "webpack --config webpack.academy.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentstack/rte-plugin-boilerplate.git"
  },
  "devDependencies": {
    "@types/node": "^10.17.5",
    "@types/react": "^16.9.11",
    "css-loader": "^6.5.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^5.0.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@contentstack/app-sdk": "^1.4.1",
    "@contentstack/venus-components": "^2.1.0",
    "@emotion/core": "^10.0.22",
    "lodash": "^4.17.21",
    "react": "^16.11.0",
    "react-dom": "^16.13.1"
  },
  "keywords": [
    "rte",
    "plugin",
    "boilerplate"
  ],
  "author": "Contentstack",
  "license": "MIT"
}
